{
  "homepage": ".",
  "main": "build/electron/main.js",
  "author": "wuji-solutions",
  "description": "React-TypeScript-Electron sample with Create React App and Electron Builder",
  "build": {
    "extends": null,
    "files": [
      "build/**/*",
      "electron/**/*",
      "!**/*.map",
      "!src/",
      "!**/.cache",
      "!**/tests",
      "!**/test",
      "!**/*.md"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      {
        "from": "backend/build/native/nativeCompile",
        "to": "backend/",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.10.0",
    "electron-is-dev": "^3.0.1",
    "gsap": "^3.13.0",
    "js-cookie": "^3.0.5",
    "konva": "^9.3.22",
    "ngraph.events": "1.2.2",
    "panzoom": "^9.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-konva": "^19.0.7",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.18",
    "tree-kill": "^1.2.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@types/electron-devtools-installer": "^4.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^4.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "prettier": "3.5.3",
    "wait-on": "^8.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "yarn build-backend:dev && yarn copy-backend && concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "build-backend": "cd ../backend && ./gradlew bootJar",
    "build-backend:dev": "cd ../backend && ./gradlew bootJar",
    "copy-backend": "cross-env NODE_ENV=development node backend/copy-backend.js",
    "copy-backend:build": "cross-env NODE_ENV=production node backend/copy-backend.js",
    "copy-backend:dev": "cross-env NODE_ENV=development node backend/copy-backend.js",
    "electron:build": "cross-env NODE_ENV=production yarn build && tsc -p electron && electron-builder",
    "dist": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ],
          "config": {}
        }
      ]
    }
  }
}
